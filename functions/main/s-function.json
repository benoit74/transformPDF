{
  "name": "main",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: transformPDF",
  "customName": false,
  "customRole": false,
  "handler": "handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "custom": {
    "optimize": {
      "exclude": ["aws-sdk"]
    }
  },
  "endpoints": [],
  "events": [
    {
      "name" : "myS3Event",
      "type": "s3",
      "config": {
        "bucket": "${stageL}-${projectL}-storage",
        "bucketEvents": ["s3:ObjectCreated:*"],
        "filterRules": [
          {
            "name":"prefix", 
            "value":"incoming_mails"
          }
        ]
      }
    }
  ],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
